<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchorPane" styleClass="root-pane" stylesheets="@style.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.calendarreminderapp.controllers.DashboardController">
   <children>
      <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" spacing="18.0" styleClass="container">
               <children>
                  <Label fx:id="dashboardLabel" alignment="CENTER" styleClass="title" text="Dashboard" />
                  <HBox alignment="CENTER" spacing="18.0">
                     <children>
                        <Button fx:id="viewRemindersButton" mnemonicParsing="false" styleClass="button-primary" text="View Reminders" />
                        <Button fx:id="calenderButton" layoutX="500.0" mnemonicParsing="false" styleClass="button-primary" text="Go To Calender View">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
